<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="bpmn-js (https://demo.bpmn.io)" exporterVersion="18.9.0">
  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="Participant_Evanstonian" name="The Evanstonian Room Service" processRef="Process_RoomService" />
  </bpmn:collaboration>
  <bpmn:process id="Process_RoomService" isExecutable="false">
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_Waiter" name="Waiter">
        <bpmn:flowNodeRef>Task_ReadyCart</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Join</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_DeliverOrder</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_DebitAccount</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_ProcessComplete</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Sommelier" name="Sommelier">
        <bpmn:flowNodeRef>Gateway_AlcoholCheck</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_FetchWine</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_AlcoholMerge</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Kitchen" name="Kitchen">
        <bpmn:flowNodeRef>Task_PrepareFood</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Manager" name="Room-Service Manager">
        <bpmn:flowNodeRef>StartEvent_OrderReceived</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_TakeOrder</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Fork</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_OrderReceived" name="Order Received">
      <bpmn:outgoing>Flow_01</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_TakeOrder" name="Take order">
      <bpmn:incoming>Flow_01</bpmn:incoming>
      <bpmn:outgoing>Flow_02</bpmn:outgoing>
    </bpmn:task>
    <bpmn:parallelGateway id="Gateway_Fork">
      <bpmn:incoming>Flow_02</bpmn:incoming>
      <bpmn:outgoing>Flow_ToKitchen</bpmn:outgoing>
      <bpmn:outgoing>Flow_ToSommelier</bpmn:outgoing>
      <bpmn:outgoing>Flow_ToWaiter</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:task id="Task_PrepareFood" name="Prepare food">
      <bpmn:incoming>Flow_ToKitchen</bpmn:incoming>
      <bpmn:outgoing>Flow_FoodReady</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_AlcoholCheck" name="Alcoholic beverages ordered?">
      <bpmn:incoming>Flow_ToSommelier</bpmn:incoming>
      <bpmn:outgoing>Flow_YesAlcohol</bpmn:outgoing>
      <bpmn:outgoing>Flow_NoAlcohol</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_FetchWine" name="Fetch wine &#38; prepare drinks">
      <bpmn:incoming>Flow_YesAlcohol</bpmn:incoming>
      <bpmn:outgoing>Flow_WineReady</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_AlcoholMerge">
      <bpmn:incoming>Flow_WineReady</bpmn:incoming>
      <bpmn:incoming>Flow_NoAlcohol</bpmn:incoming>
      <bpmn:outgoing>Flow_DrinksReady</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_ReadyCart" name="Ready cart &#38; nonalcoholic drinks">
      <bpmn:incoming>Flow_ToWaiter</bpmn:incoming>
      <bpmn:outgoing>Flow_CartReady</bpmn:outgoing>
    </bpmn:task>
    <bpmn:parallelGateway id="Gateway_Join">
      <bpmn:incoming>Flow_FoodReady</bpmn:incoming>
      <bpmn:incoming>Flow_DrinksReady</bpmn:incoming>
      <bpmn:incoming>Flow_CartReady</bpmn:incoming>
      <bpmn:outgoing>Flow_03</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:task id="Task_DeliverOrder" name="Deliver order to guest">
      <bpmn:incoming>Flow_03</bpmn:incoming>
      <bpmn:outgoing>Flow_04</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_DebitAccount" name="Debit guest&#39;s account">
      <bpmn:incoming>Flow_04</bpmn:incoming>
      <bpmn:outgoing>Flow_05</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="EndEvent_ProcessComplete" name="Process Complete">
      <bpmn:incoming>Flow_05</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_01" sourceRef="StartEvent_OrderReceived" targetRef="Task_TakeOrder" />
    <bpmn:sequenceFlow id="Flow_02" sourceRef="Task_TakeOrder" targetRef="Gateway_Fork" />
    <bpmn:sequenceFlow id="Flow_ToKitchen" sourceRef="Gateway_Fork" targetRef="Task_PrepareFood" />
    <bpmn:sequenceFlow id="Flow_ToSommelier" sourceRef="Gateway_Fork" targetRef="Gateway_AlcoholCheck" />
    <bpmn:sequenceFlow id="Flow_ToWaiter" sourceRef="Gateway_Fork" targetRef="Task_ReadyCart" />
    <bpmn:sequenceFlow id="Flow_FoodReady" sourceRef="Task_PrepareFood" targetRef="Gateway_Join" />
    <bpmn:sequenceFlow id="Flow_YesAlcohol" name="Yes (80%)" sourceRef="Gateway_AlcoholCheck" targetRef="Task_FetchWine" />
    <bpmn:sequenceFlow id="Flow_NoAlcohol" name="No (20%)" sourceRef="Gateway_AlcoholCheck" targetRef="Gateway_AlcoholMerge" />
    <bpmn:sequenceFlow id="Flow_WineReady" sourceRef="Task_FetchWine" targetRef="Gateway_AlcoholMerge" />
    <bpmn:sequenceFlow id="Flow_DrinksReady" sourceRef="Gateway_AlcoholMerge" targetRef="Gateway_Join" />
    <bpmn:sequenceFlow id="Flow_CartReady" sourceRef="Task_ReadyCart" targetRef="Gateway_Join" />
    <bpmn:sequenceFlow id="Flow_03" sourceRef="Gateway_Join" targetRef="Task_DeliverOrder" />
    <bpmn:sequenceFlow id="Flow_04" sourceRef="Task_DeliverOrder" targetRef="Task_DebitAccount" />
    <bpmn:sequenceFlow id="Flow_05" sourceRef="Task_DebitAccount" targetRef="EndEvent_ProcessComplete" />
  </bpmn:process>
